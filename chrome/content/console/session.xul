<?xml version="1.0"?>

<overlay xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <tabpanel id="session">     
    <vbox flex="1">
      <hbox>
        <label value="Session log :"
               flex="1"/>
        <button label="Refresh"
                oncommand="console.fill_console('session')"/>
      </hbox>
      <textbox id="sessionLog"
               flex="1"
               multiline="true"/>
    </vbox>
  </tabpanel>
</overlay>