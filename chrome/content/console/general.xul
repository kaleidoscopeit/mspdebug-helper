<?xml version="1.0"?>

<overlay xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <tabpanel id="main">     
    <vbox flex="1">
      <hbox>
        <label value="Main log :"
               flex="1"/>
        <button label="Refresh"
                oncommand="console.fill_console('main')"/>
      </hbox>
      <textbox id="mainLog"
               flex="1"
               multiline="true"/>
    </vbox>
  </tabpanel>
</overlay>