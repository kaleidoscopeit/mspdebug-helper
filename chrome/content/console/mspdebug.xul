<?xml version="1.0"?>

<overlay xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">

  <tabpanel id="mspdebug">     
    <vbox flex="1">
      <hbox>
        <label value="MSPDebug log :"
               flex="1"/>
        <button label="Refresh"
                oncommand="console.fill_console('gdb')"/>
      </hbox>
      <textbox id="gdbLog"
               flex="1"
               multiline="true"/>
    </vbox>
  </tabpanel>
</overlay>