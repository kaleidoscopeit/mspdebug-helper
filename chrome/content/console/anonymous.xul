<?xml version="1.0"?>

<overlay xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">

  <tabpanel id="anonymous">     
    <vbox flex="1">
      <hbox>
        <label value="Anonymous log :"
               flex="1"/>
        <button label="Refresh"
                oncommand="console.fill_console('anonymous')"/>
      </hbox>
      <textbox id="anonymousLog"
               flex="1"
               multiline="true"/>
    </vbox>
  </tabpanel>
</overlay>